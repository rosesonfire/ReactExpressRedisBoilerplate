{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": ["standard", "standard-react"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        },
        "sourceType": "module",
        "ecmaVersion": 2017
    },
    "rules": {
        "no-console": "error"
    }
}