{
  "name": "ReactExpressRedisBoilerplate",
  "version": "0.0.1",
  "description": "ReactExpressRedisBoilerplate",
  "main": "index.js",
  "config": {
    "src_dir": "src",
    "prod_dir": "dist",
    "src_dir_js_files": "src/**/*.js",
    "back_end_dir": "dist/backEnd/main",
    "front_end_dir": "dist/frontEnd/main",
    "db_dir": "dist/db",
    "src_db_dir": "src/db",
    "migrate_mongoose_config": "dist/db/migrate.conf.js",
    "webpack_config": "dist/frontEnd/main/webpack.config.js",
    "back_end_files": "dist/backEnd/main/**/*",
    "front_end_files": "dist/frontEnd/main/**/*",
    "back_end_test_files": "dist/backEnd/test/**/*.spec.js",
    "front_end_test_files": "dist/frontEnd/test/**/*.spec.js",
    "back_end_coverage_dir": "coverage/backEnd",
    "front_end_coverage_dir": "coverage/frontEnd",
    "back_end_coverage": "coverage/backEnd/index.html",
    "front_end_coverage": "coverage/frontEnd/index.html",
    "back_end_ioc_files": "dist/backEnd/main/ioc/**/*.js",
    "back_end_index_file": "dist/backEnd/main/index.js",
    "back_end_config_files": "dist/backEnd/main/config/**/*.js"
  },
  "scripts": {
    "clean": "rm -rf $npm_package_config_prod_dir && mkdir $npm_package_config_prod_dir",
    "build": "babel $npm_package_config_src_dir -d $npm_package_config_prod_dir --copy-files",
    "clean-build": "npm run clean && npm run build",
    "build-with-watch": "babel $npm_package_config_src_dir --watch -d $npm_package_config_prod_dir --copy-files",
    "list-migrations": "npm run clean-build && migrate --config $npm_package_config_migrate_mongoose_config --change-dir $npm_package_config_db_dir list",
    "create-migration": "npm run clean-build && migrate --config $npm_package_config_migrate_mongoose_config --change-dir $npm_package_config_src_db_dir create $1",
    "prune-migrations": "npm run clean-build && migrate --config $npm_package_config_migrate_mongoose_config --change-dir $npm_package_config_db_dir prune",
    "migrate": "npm run clean-build && migrate --config $npm_package_config_migrate_mongoose_config --change-dir $npm_package_config_db_dir up",
    "migrate-down": "npm run clean-build && migrate --config $npm_package_config_migrate_mongoose_config --change-dir $npm_package_config_db_dir down $1",
    "start-back-end": "nodemon --watch $npm_package_config_back_end_dir $npm_package_config_back_end_dir",
    "start-front-end": "webpack-dev-server --watch --config $npm_package_config_webpack_config --context $npm_package_config_front_end_dir",
    "start-servers-and-watchers": "npm run start-back-end & npm run start-front-end & npm run build-with-watch",
    "start": "npm run clean-build && npm run start-servers-and-watchers",
    "eslint-test": "standard --parser babel-eslint \"$npm_package_config_src_dir_js_files\"",
    "eslint-fix": "standard --parser babel-eslint --fix \"$npm_package_config_src_dir_js_files\"",
    "test-back-end": "nyc --reporter=html --report-dir=$npm_package_config_back_end_coverage_dir --all --include=$npm_package_config_back_end_files --exclude=$npm_package_config_back_end_ioc_files --exclude=$npm_package_config_back_end_config_files --exclude=$npm_package_config_back_end_index_file mocha \"$npm_package_config_back_end_test_files\" && sensible-browser $npm_package_config_back_end_coverage",
    "test-front-end": "nyc --reporter=html --report-dir=$npm_package_config_front_end_coverage_dir --all --include=$npm_package_config_front_end_files mocha \"$npm_package_config_front_end_test_files\" && sensible-browser $npm_package_config_front_end_coverage",
    "test-both-ends": "npm run test-back-end & npm run test-front-end",
    "test": "npm run eslint-test && npm run clean-build && npm run test-both-ends"
  },
  "author": "Ishtiaque Khan",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-arrays": "^2.0.0",
    "colors": "^1.1.2",
    "electrolyte": "^0.5.0",
    "express": "^4.16.2",
    "migrate-mongoose": "^3.2.2",
    "mocha": "^4.1.0",
    "mongodb": "^2.2.35",
    "mongoose": "^4.13.11",
    "node": "^8.9.4",
    "passport": "^0.4.0",
    "passport-accesstoken": "^0.1.0",
    "react": "^16.2.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^4.4.1",
    "redux-thunk": "^2.2.0",
    "sinon": "^4.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-exclude-assets-plugin": "0.0.5",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "nodemon": "^1.17.1",
    "nyc": "^11.5.0",
    "on-build-webpack": "^0.1.0",
    "sass-loader": "^6.0.6",
    "standard": "^11.0.0",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.2"
  }
}
